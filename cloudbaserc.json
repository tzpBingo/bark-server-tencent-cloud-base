{
  "version": "2.0",
  "envId": "{{env.ENV_ID}}",
  "framework": {
    "name": "bark",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-container",
        "inputs": {
          "serviceName": "bark",
          "servicePath": "/",
          "uploadType": "image",
          "containerPort": 8080,
          "imageInfo": {
            "imageUrl": "finab/bark-server:latest"
          },
          "envVariables": {},
          "volumeMounts": {
            "/data": "bark-cfs"
          }
        }
      }
    },
    "requirement": {
      "addons": [
        {
          "type": "CFS",
          "name": "bark-cfs"
        }
      ]
    }
  }
}
